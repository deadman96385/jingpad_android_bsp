		fgu-mode = <0>;
		alm-soc = <5>;
		alm-vol = <3600>;
		soft-vbat-uvlo = <3100>;
		rint = <250>;
		cnom = <1900>;
		rsense-real = <215>;
		rsense-spec = <200>;
		relax-current = <50>;
		fgu-cal-ajust = <0>;
		ocv-tab-vol = <4185 4113 4066 4022 3983 3949 3917
				3889 3864 3835 3805 3787 3777 3773
				3770 3765 3752 3724 3680 3605 3400>;
