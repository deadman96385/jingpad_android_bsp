/* This is the sprd battery default DTS */
/{
	battery: battery {
		compatible = "sprd,sprd-battery";
		status = "disabled";
		chg-end-vol = <4200>;
		chg-end-vol-check = <4190>;
		chg-bat-safety-vol = <4280>;
		fchg-vol = <9000>;
		rechg-vol = <4131>;
		adp-cdp-cur = <700>;
		adp-dcp-cur = <700>;
		adp-sdp-cur = <450>;
		adp-fchg-cur = <2500>;
		ovp-stop = <6500>;
		ovp-restart = <5800>;
		fchg-ovp-stop = <11000>;
		fchg-ovp-restart = <10000>;
		chg-timeout = <21600>;
		chg-rechg-timeout = <5400>;
		chg-end-cur = <80>;
		chg-polling-time = <15>;
		chg-polling-time-fast = <1>;
		cap-one-per-time = <30>;
		/*0 vol and cur,1 status and cur,2 ext ic*/
		chg-full-condition = <0>;
		temp-support = <0>;
		temp-table-mode = <1>;
		/*30mohm,if temp-table-mode = <1>, use it*/
